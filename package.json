	"bugs": {
		"url": "https://github.com/es-shims/Array.prototype.groupByToMap/issues"
	},
	"homepage": "https://github.com/es-shims/Array.prototype.groupByToMap#readme",
	"devDependencies": {
		"@es-shims/api": "^2.2.3",
		"@ljharb/eslint-config": "^20.1.0",
		"aud": "^1.1.5",
